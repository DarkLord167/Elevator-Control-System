SCADE=C:\Program Files\ANSYS Inc\v212\SCADE\SCADE\.

SHELL=cmd.exe

C_FLAGS = -pedantic

all: ControlSystem.exe ControlSystem.dll 

win64/ControlSystem.o: ControlSystem.c
	gcc -I"$(SCADE)\libraries\SC65\libmathext" -I"." -I"$(SCADE)" -I"$(SCADE)\include" -I"$(SCADE)\include\C" -I"$(SCADE)\include\Ada" -I"$(SCADE)\lib\Ada" $(C_FLAGS) -DBUILD_DLL -DSIM -DWIN32 -D_CONSOLE -D_MBCS -c -ansi -std=c99 -m64 "ControlSystem.c" -o "win64\ControlSystem.o"

